{
    "name": "yieldstudio/nova-phone-field",
    "description": "Nova Phone Number field with a dynamic mask based on the country code inserted by the user.",
    "keywords": [
        "yieldstudio",
        "laravel",
        "nova",
        "phone mask",
        "input mask"
    ],
    "homepage": "https://github.com/YieldStudio/nova-phone-field",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Bissolli",
            "email": "gustavo.bissolli@gmail.com",
            "homepage": "https://github.com/bissolli",
            "role": "Creator"
        },
        {
            "name": "James Hemery",
            "email": "james@yieldstudio.fr",
            "homepage": "https://yieldstudio.fr",
            "role": "Fork maintener"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "YieldStudio\\NovaPhoneField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YieldStudio\\NovaPhoneField\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.10"
    }
}
